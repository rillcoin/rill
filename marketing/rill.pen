                  "textGrowth": "fixed-width",
                  "width": "fill_container",
                  "content": "// Progressive concentration decay\nfn apply_decay(balance: u64, threshold: u64) -> u64 {\n    if balance <= threshold {\n        return balance;\n    }\n    let excess = balance\n        .checked_sub(threshold)\n        .unwrap_or(0);\n    let rate = sigmoid_rate(excess, threshold);\n    let decay = excess\n        .checked_mul(rate)\n        .unwrap_or(0)\n        .checked_div(100_000_000)\n        .unwrap_or(0);\n    balance.saturating_sub(decay)\n}",
